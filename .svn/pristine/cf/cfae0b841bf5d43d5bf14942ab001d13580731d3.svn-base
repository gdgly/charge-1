package com.iycharge.server.domain.entity.utils;

import java.util.Arrays;
import java.util.List;

public enum Gender {
    FEMALE("女"), MALE("男");

    private String title;

    Gender(String title) {
        this.title = title;
    }

    public String getTitle() {
        return title;
    }

    @Override
    public String toString() {
        return this.title;
    }

    public static List<Gender> asList() {
        return Arrays.asList(FEMALE, MALE);
    }
}
